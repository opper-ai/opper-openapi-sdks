{
  "name": "opper-openapi-sdks",
  "version": "0.1.0",
  "description": "Generate SDKs from OpenAPI specs using AI agents",
  "type": "module",
  "bin": {
    "opper-openapi-sdks": "./dist/cli.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsx src/cli.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "tsc --noEmit"
  },
  "keywords": [
    "openapi",
    "sdk",
    "code-generation",
    "llm",
    "opper",
    "ai-agents"
  ],
  "license": "MIT",
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.1",
    "@opperai/agents": "^0.8.0",
    "commander": "^13.1.0",
    "openapi-types": "^12.1.3",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "tsup": "^8.4.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "vitest": "^3.0.0"
  }
}
